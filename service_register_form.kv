<ServiceRegister>:
    name: 'service_register_form'
    BoxLayout:
        orientation: 'vertical'

        MDIconButton:
            icon: "arrow-left"
            pos_hint: {"center_y": .95}
            user_font_size: "30sp"
            theme_text_color: "Custom"
            text_color: rgba(26, 24, 58, 255)
            on_release:
                root.manager.transition.direction = "right"
                root.manager.current = "service_provider"
        MDLabel:
            text: "Create new account"
            font_name: "Roboto"
            pos_hint: {"center_x":.6,"center_y": .95}
            size_hint_y:.1
            font_size: "18sp"
        ScrollView:
            GridLayout:
                id: grid
                cols: 1
                padding: dp(10)
                spacing: dp(20)
                size_hint_y: None
                height: self.minimum_height
                MDBoxLayout:
                    orientation: 'vertical'
                    size_hint_y: None
                    height: self.minimum_height
                    MDTextField:
                        id: service_provider_name
#                        required: True
                        hint_text: 'Name'
                        font_name: "Roboto"
                        pos_hint:{'center_x':.5}
                        size_hint:.9,None
                        height: "25dp"
                        multiline: False
                        color_mode: 'custom'
                        line_color_focus: 0, 0, 0, 1
                        text_color_focus:0,0,0,1
                        hint_text_color:0,0,0,1
                        hint_text_color_focus:0,0,0,.5


                    MDTextField:
                        id:service_provider_email
                        hint_text: "Email"
#                        required: True
                        font_name: "Roboto"
                        pos_hint:{'center_x':.5}
                        size_hint:.9,None
                        multiline: False
                        color_mode: 'custom'
                        line_color_focus: 0, 0, 0, 1
                        text_color_focus:0,0,0,1
                        hint_text_color:0,0,0,1
                        hint_text_color_focus:0,0,0,.5


                    MDTextField:
                        id:service_provider_password
                        hint_text: "Enter Password"
#                        required: True
                        password: True
                        font_name: "Roboto"
                        pos_hint:{'center_x':.5}
                        size_hint:.9,None
                        height: "30dp"
                        multiline: False
                        color_mode: 'custom'
                        line_color_focus: 0, 0, 0, 1
                        text_color_focus:0,0,0,1
                        hint_text_color:0,0,0,1
                        hint_text_color_focus:0,0,0,.5


                    MDTextField:
                        id:service_provider_phoneno
                        hint_text: "Phone No"
#                       required: True
                        font_name: "Roboto"
                        pos_hint:{'center_x':.5}
                        size_hint:.9,None
                        height: "30dp"
                        multiline: False
                        color_mode: 'custom'
                        line_color_focus: 0, 0, 0, 1
                        text_color_focus:0,0,0,1
                        hint_text_color:0,0,0,1
                        hint_text_color_focus:0,0,0,.5

                    MDTextField:
                        id:service_provider_address
                        hint_text: "Address"
#                        required: True
                        font_name: "Roboto"
                        pos_hint:{'center_x':.5}
                        size_hint:.9,None
                        height: "30dp"
                        multiline: False
                        color_mode: 'custom'
                        line_color_focus: 0, 0, 0, 1
                        text_color_focus:0,0,0,1
                        hint_text_color:0,0,0,1
                        hint_text_color_focus:0,0,0,.5


                    MDTextField:
                        id: dropdown_nation
                        hint_text: "Nation"
                        pos_hint: {'center_x': 0.5}
                        on_focus: root.open_dropdown(self)
                        font_size: "14sp"
                        size_hint:.9,None
                        cursor_width: "2sp"
                        readonly: True
                        theme_text_color: "Custom"
                        icon_right: 'chevron-down'
                        color_mode: 'custom'
                        line_color_focus: 0, 0, 0, 1
                        text_color_focus:0,0,0,1
                        hint_text_color:0,0,0,1
                        hint_text_color_focus:0,0,0,.5

                    MDTextField:
                        id: dropdown_state
                        hint_text: "State"
                        pos_hint: {'center_x': 0.5}
                        on_focus: root.open_dropdown2(self)
                        font_size: "14sp"
                        size_hint:.9,None
                        cursor_width: "2sp"
                        readonly: True
                        theme_text_color: "Custom"
                        icon_right: 'chevron-down'
                        color_mode: 'custom'
                        line_color_focus: 0, 0, 0, 1
                        text_color_focus:0,0,0,1
                        hint_text_color:0,0,0,1
                        hint_text_color_focus:0,0,0,.5
                        icon_right_color:0,0,0,1
                        icon_right_color_focus:0,0,0,.5


                    MDTextField:
                        id:service_provider_pincode
                        hint_text: "PIN Code"
#                        required: True
                        font_name: "Roboto"
                        font_size: "14sp"
                        pos_hint:{'center_x':.5}
                        size_hint:.9,None
                        height: "30dp"
                        multiline: False
                        color_mode: 'custom'
                        line_color_focus: 0, 0, 0, 1
                        text_color_focus:0,0,0,1
                        hint_text_color:0,0,0,1
                        hint_text_color_focus:0,0,0,.5
                    MDTextField:
                        id:extra_info
                        hint_text: "Hospital Name"
#                        required: True
                        font_name: "Roboto"
                        font_size: "14sp"
                        pos_hint:{'center_x':.5}
                        size_hint:.9,None
                        height: "30dp"
                        multiline: False
                        color_mode: 'custom'
                        line_color_focus: 0, 0, 0, 1
                        text_color_focus:0,0,0,1
                        hint_text_color:0,0,0,1
                        hint_text_color_focus:0,0,0,.5
                    MDTextField:
                        id:extra_info2
                        hint_text: "Established Year"
#                        required: True
                        font_name: "Roboto"
                        font_size: "14sp"
                        pos_hint:{'center_x':.5}
                        size_hint:.9,None
                        height: "30dp"
                        multiline: False
                        color_mode: 'custom'
                        line_color_focus: 0, 0, 0, 1
                        text_color_focus:0,0,0,1
                        hint_text_color:0,0,0,1
                        hint_text_color_focus:0,0,0,.5
                        on_focus: root.show_date_picker(self)



                    BoxLayout:
                        orientation: 'vertical'
                        spacing: 0
                        size_hint_y: None
                        height: dp(60)
                        md_bg_color: 1, 1, 1, 1

                        MDLabel:
                            text:'Upload Required Documents'
                            size_hint_y:.1
                            halign:'center'
                            bold: True
                            font_size: "16sp"
                            pos_hint: {"top": 1, "center_x": 0.5}

                    BoxLayout:
                        orientation: 'vertical'
                        spacing: 0
                        size_hint_y: None
                        height: dp(60)
                        padding: dp(20)
                        spacing: dp(10)
                        md_bg_color: 1, 1, 1, 1

                        MDLabel:
                            text: "Medical Practitioner License"
                            size_hint_y: 1
                            font_name: "Roboto"
                            halign: "left"
#                                valign: "top"
                            color: 0, 0, 0, 1

                        BoxLayout:
                            orientation: 'horizontal'
                            size_hint_y: None
                            height: self.minimum_height
                            pos_hint: {"center_x": .5,'center_y':.3}

                            MDFlatButton:
                                size_hint: None, None
                                size: dp(150), dp(35)  # Adjusted size
                                halign: "left"
                                md_bg_color: 0, 0, 0, 0
                                on_release: root.file_manager_open()

                                BoxLayout:
                                    orientation: 'horizontal'
                                    MDLabel:
                                        id: file_selected_1
                                        text: "Select file"
                                        italic: True
                                        multiline: False
                                        text_color: 0, 0, 0, 1
                                        theme_text_color: "Custom"

                                        canvas.before:
                                            Color:
                                                rgba: 0.5, 0.5, 0.5, 1
                                            Line:
                                                points: self.x, self.y, self.x + self.width, self.y
                                                width: 1

                                    MDIcon:
                                        icon: "file-upload"
                                        theme_text_color: "Custom"
                                        text_color: 1, 0, 0, 1
                                        size_hint_x: None
                                        width: dp(24)

                            MDFlatButton:
                                id: upload
                                text: "UPLOAD"
                                size_hint:.4,.5
                                pos_hint: {"center_x": .8,'center_y':.5}
                                background_color: 0, 0, 0, 0
                                font_name: "Roboto"
                                font_size: "12sp"
                                background_color: 0, 0, 0, 0
                                color: rgba(1, 1, 1, 255)
                                on_release: root.upload_file()
                                canvas.before:
                                    Color:
                                        rgb: rgba(255, 0, 0, 255)
                                    Line:
                                        width: 1.2
                                        rounded_rectangle: self.x, self.y, self.width, self.height, 5, 5, 5, 5, 100

                    BoxLayout:
                        orientation: 'vertical'
                        spacing: 0
                        size_hint_y: None
                        height: dp(60)
                        padding: dp(20)
                        spacing: dp(10)
                        md_bg_color: 1, 1, 1, 1

                        MDLabel:
                            text: "Building Permit and Licenses"
                            size_hint_y: 1
                            font_name: "Roboto"
                            halign: "left"
#                                valign: "top"
                            color: 0, 0, 0, 1

                        BoxLayout:
                            orientation: 'horizontal'
                            size_hint_y: None
                            height: self.minimum_height
                            pos_hint: {"center_x": .5,'center_y':.3}


                            MDFlatButton:
                                size_hint: None, None
                                size: dp(150), dp(35)  # Adjusted size
                                halign: "left"
                                md_bg_color: 0, 0, 0, 0
                                on_release: root.file_manager_open()

                                BoxLayout:
                                    orientation: 'horizontal'
                                    MDLabel:
                                        id: file_selected_2
                                        text: "Select file"
                                        italic: True
                                        multiline: False
                                        text_color: 0, 0, 0, 1
                                        theme_text_color: "Custom"

                                        canvas.before:
                                            Color:
                                                rgba: 0.5, 0.5, 0.5, 1
                                            Line:
                                                points: self.x, self.y, self.x + self.width, self.y
                                                width: 1

                                    MDIcon:
                                        icon: "file-upload"
                                        theme_text_color: "Custom"
                                        text_color: 1, 0, 0, 1
                                        size_hint_x: None
                                        width: dp(24)

                            MDFlatButton:
                                id: upload
                                text: "UPLOAD"
                                size_hint:.4,.5
                                pos_hint: {"center_x": .8,'center_y':.5}
                                background_color: 0, 0, 0, 0
                                font_name: "Roboto"
                                font_size: "12sp"
                                background_color: 0, 0, 0, 0
                                color: rgba(1, 1, 1, 255)
                                on_release: root.upload_file()
                                canvas.before:
                                    Color:
                                        rgb: rgba(255, 0, 0, 255)
                                    Line:
                                        width: 1.2
                                        rounded_rectangle: self.x, self.y, self.width, self.height, 5, 5, 5, 5, 100

                    BoxLayout:
                        orientation: 'vertical'
                        size_hint_y: None
                        height: self.minimum_height
                        padding: dp(20)
                        spacing: dp(10)
                        # signup button
                    Button:
                        id: service_register
                        text: "REGISTER"
                        size_hint: .7,None
                        height:dp(40)
                        pos_hint: {"center_x": .5, "center_y": .34}
                        background_color: 0, 0, 0, 0
                        font_name: "Roboto"
                        on_release:
                            root.registration_submit_button(self)
                        canvas.before:
                            Color:
                                rgb: rgba(255, 0, 0, 255)
                            RoundedRectangle:
                                size: self.size
                                pos: self.pos
                                radius: [5]