<HorizontalLineWidget>:
    canvas:
        Color:
            rgba: 0.5, 0.5, 0.5, 1  # Set the color of the line (Blue in this example)
        Line:
            points: self.x, self.center_y, self.right, self.center_y  # Define the coordinates of the line
<HospitalContent>
    name:'hospital_content'
    BoxLayout:
        orientation: 'vertical'
        MDLabel:
            text: "Hospital "
            font_name: "Broboto"
            pos_hint: {"center_x":.6,"center_y": .95}
            size_hint_y:.1
            font_size: "18sp"
        ScrollView:
            GridLayout:
                id: grid
                cols: 1
                padding: dp(10)
                spacing: dp(20)
                size_hint_y: None
                height: self.minimum_height
                MDBoxLayout:
                    orientation: 'vertical'
                    size_hint_y: None
                    height: self.minimum_height



                    MDTextField:
                        id:extra_info
                        hint_text: "Hospital Name"
            #                        required: True
                        font_name: "Roboto"
                        font_size: "14sp"
                        pos_hint:{'center_x':.5}
                        size_hint:.7,None
                        write_tab: False
                        height: "30dp"
                        multiline: False
                        color_mode: 'custom'
                        line_color_focus: 0, 0, 0, 1
                        text_color_focus:0,0,0,1
                        hint_text_color:0,0,0,1
                        hint_text_color_focus:0,0,0,.5
                    MDTextField:
                        id:extra_info2
                        hint_text: "Established Year"
            #                        required: True
                        font_name: "Roboto"
                        font_size: "14sp"
                        pos_hint:{'center_x':.5}
                        size_hint:.7,None
                        write_tab: False
                        height: "30dp"
                        multiline: False
                        color_mode: 'custom'
                        line_color_focus: 0, 0, 0, 1
                        text_color_focus:0,0,0,1
                        hint_text_color:0,0,0,1
                        hint_text_color_focus:0,0,0,.4
                        on_focus: if self.focus: root.show_date_picker(self)
                    MDTextField:
                        id: State
                        hint_text: "State"
                        pos_hint: {'center_x': 0.5}
                        font_size: "14sp"
                        size_hint:.7,None
                        cursor_width: "2sp"
                        write_tab: False
                        theme_text_color: "Custom"
                        color_mode: 'custom'
                        line_color_focus: 0, 0, 0, 1
                        text_color_focus:0,0,0,1
                        hint_text_color:0,0,0,1
                        hint_text_color_focus:0,0,0,.5
                    MDTextField:
                        id: District
                        hint_text: "District"
                        pos_hint: {'center_x': 0.5}
                        font_size: "14sp"
                        size_hint:.7,None
                        cursor_width: "2sp"
                        write_tab: False
                        theme_text_color: "Custom"
                        color_mode: 'custom'
                        line_color_focus: 0, 0, 0, 1
                        text_color_focus:0,0,0,1
                        hint_text_color:0,0,0,1
                        hint_text_color_focus:0,0,0,.5
                    MDTextField:
                        id:pincode
                        hint_text: "PIN Code"
            #                        required: True
                        font_name: "Roboto"
                        font_size: "14sp"
                        pos_hint:{'center_x':.5}
                        size_hint:.7,None
                        height: "30dp"
                        multiline: False
                        write_tab: False
                        color_mode: 'custom'
                        line_color_focus: 0, 0, 0, 1
                        text_color_focus:0,0,0,1
                        hint_text_color:0,0,0,1
                        hint_text_color_focus:0,0,0,.5
                    MDTextField:
                        id:address
                        hint_text: "Address"
            #                        required: True
                        font_name: "Roboto"
                        font_size: "14sp"
                        pos_hint:{'center_x':.5}
                        size_hint:.7,None
                        height: "30dp"
                        multiline: False
                        write_tab: False
                        color_mode: 'custom'
                        line_color_focus: 0, 0, 0, 1
                        text_color_focus:0,0,0,1
                        hint_text_color:0,0,0,1
                        hint_text_color_focus:0,0,0,.5
                    Button:
                        id: register_submit
                        text: "SUBMIT"
                        size_hint: .4,None
                        height:dp(30)
                        pos_hint: {"center_x": .5, "center_y": 1}
                        background_color: 0, 0, 0, 0
                        font_name: "Roboto"
                        on_release:
                            root.manager.transition.direction = "down"
                            root.manager.current = "register_page2"
                        canvas.before:
                            Color:
                                rgb: rgba(255, 0, 0, 255)
                            RoundedRectangle:
                                size: self.size
                                pos: self.pos
                                radius: [5]




