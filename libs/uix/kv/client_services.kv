                                id: md_list
                                MDList:
                                    OneLineIconListItem:
                                        text: "Home"
                                        on_release: root.home()
                                        IconLeftWidget:
                                            icon: "home"
                                            on_release: root.home()
                                    OneLineIconListItem:
                                        text: "Profile"
                                        on_release: root.manager.push("menu_profile")
                                        IconLeftWidget:
                                            icon: "account-circle"
                                            on_release: root.manager.push("menu_profile")
                                    OneLineIconListItem:
                                        text: "Notification"
                                        on_release: root.manager.push("menu_notification")
                                        IconLeftWidget:
                                            icon: "bell-outline"
                                            on_release: root.manager.push("menu_notification")

                                    OneLineIconListItem:
                                        text: "Bookings"
                                        on_release: root.manager.push("menu_bookings")
