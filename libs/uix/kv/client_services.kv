                    id: nav_drawer
                    orientation: 'vertical'
                    size_hint_x:dp(290)
                    size_hint_max_x: "270dp"
                    padding: "0dp"
                    spacing: "5dp"
                    MDCard:
                        orientation: 'vertical'
                        size_hint: None, None
                        size: "268dp", "100dp"
                        elevation: 2
                        md_bg_color: 1, 1, 1, 1
                        BoxLayout:
                            size_hint_y: None
                            height: dp(90)
                            padding: "15dp"
                            BoxLayout:
                                size_hint: None, None
                                size: dp(60), dp(60)
                                canvas:
                                    Color:
                                        rgb: 1, 1, 1
                                    Ellipse:
                                        pos: self.pos
                                        size: 60, 60
                                        source: 'images/doctor.jpg'
                                        angle_start: 0
                                        angle_end: 360
                            BoxLayout:
                                orientation: 'vertical'
                                padding: "5dp"
                                spacing: "5dp"
                                MDLabel:
                                    id: username
                                    text: "username"
                                    font_name: "Roboto"
                                    theme_text_color: "Secondary"
                                    pos_hint: {'center_x': 0.5,'center_y': 0.2}
                                MDLabel:
                                    id: email
                                    text: "clientemail@gmail.com"
                                    theme_text_color: "Hint"
                                    font_name: "Roboto"
                                    font_size:'12sp'
                                    pos_hint: {'center_x': 0.5,'center_y': 0.1}

                    BoxLayout:
                        orientation: 'vertical'
                        ScrollView:
