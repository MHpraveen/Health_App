<Client_services>:
    ScreenManager:
        id: screen_manager
        NavigationDrawerScreen:
            name: "navigation_drawer"
            MDScreen:
                BoxLayout:
                    orientation: 'vertical'
                    MDTopAppBar:
                        title: "Available Services"
                        font_name: "Roboto"
                        md_bg_color: 1, 0, 0, 1
                        left_action_items: [["menu", lambda x: nav_drawer.set_state()]]
                        right_action_items: [["map-marker", lambda x: root.location_screen()]]
                    ScrollView:
                        GridLayout:
                            id: card_grid
                            cols: 1
                            spacing: dp(10)
                            padding: dp(10)
                            size_hint_y: None
                            height: self.minimum_height
                            pos_hint: {"center_x": 0.5, "center_y": 0.5}
                            BoxLayout:
                                orientation: 'vertical'
                                size_hint_y: None
                                height: dp(185)
                                md_bg_color: 1, 1, 1, 1

                                MDCard:
                                    orientation: 'horizontal'
                                    size_hint: (1, 1)
                                    elevation: 2
                                    pos_hint: {"center_x": 0.5, "center_y": 0.5}
                                    BoxLayout:
                                        orientation: 'vertical'
                                        size_hint: (0.5, 1)
                                        padding: dp(8),dp(5),dp(5),dp(5)
                                        AsyncImage:
                                            id: organization_image
                                            source: 'images/hospital-building.jpg'
                                            size_hint: (1, 1)
                                            pos_hint: {'center_x': 0.5, 'center_y': 0.5}
                                            allow_stretch: True

                                    BoxLayout:
                                        id: information
                                        orientation: 'vertical'
                                        size_hint_x: 0.5
                                        padding: dp(2)
                                        spacing: dp(10)
                                        BoxLayout:
                                            id: box_one
                                            orientation: 'horizontal'
                                            size_hint_y: None
                                            height: dp(50)
                                            spacing: dp(5)
                                            pos_hint: {'top': 1}

                                            MDLabel:
                                                id: Horganization_name
                                                text: "Hospital"
                                                font_name: "Roboto-Bold"
                                                theme_text_color: "Primary"
                                                font_size: '25sp'
                                                halign: 'center'
                                                valign: 'center'
                                                height: dp(50)
                                                pos_hint: {'center_x': 0.5, 'center_y': 0.5}
                                                canvas.before:
                                                    Color:
                                                        rgba: 0.5, 0.5, 0.5, 1
                                                    Line:
                                                        points: self.x, self.y, self.x + self.width, self.y
                                                        width: 1

                                        BoxLayout:
                                            id: box_two
                                            orientation: 'horizontal'
                                            size_hint_y: None
                                            height: dp(70)
                                            spacing: dp(5)
                                            MDLabel:
                                                id: Horganization_address
                                                text: "Hospitals Address"
                                                font_name: "Roboto"
                                                theme_text_color: "Primary"
                                                font_size: '18sp'
                                                pos_hint: {'center_x': 0.5,'center_y': 0.8}

                                        MDRaisedButton:
                                            text: "Book Now"
                                            font_name: "Roboto-Bold"
                                            size_hint: None, None
                                            size: "100dp", "30dp"
                                            pos_hint: {'right': 0.95, 'y': 0}
                                            md_bg_color: 1, 0, 0, 1
                                            elevation: 3
                                            theme_text_color: "Custom"
                                            text_color: 1, 1, 1, 1
                                            font_style: "Button"
                                            on_release: root.book_now( Horganization_name.text, Horganization_address.text)
                                            canvas.before:
                                                Color:
                                                    rgb: rgba(255, 0, 0, 255)
                                                RoundedRectangle:
                                                    size: self.size
                                                    pos: self.pos
