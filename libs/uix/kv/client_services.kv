                                    OneLineIconListItem:
                                        text: "Reports"
                                        on_release: root.manager.push("menu_reports")
                                        IconLeftWidget:
                                            icon: "file-multiple"
                                            on_release: root.manager.push("menu_reports")

                                    OneLineIconListItem:
                                        text: "Support"
                                        on_release: root.manager.push("menu_support")
                                        IconLeftWidget:
                                            icon: "book-information-variant"
